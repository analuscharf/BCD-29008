# --------------------------- Senha do Aluno --------------------------------------------------#

#  Acesso restrito aos alunos por meio de autenticação (matrícula/cpf + senha).

# Alterando a tabela
alter table aluno modify cpf varchar(50) not null;
alter table aluno modify senha varchar(20) not null;

# Pondo senha para todos. 
update aluno set senha = '123456' where senha is null; 
select * from aluno;

